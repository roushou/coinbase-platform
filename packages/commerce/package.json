{
  "name": "@coinbase-platform/commerce",
  "version": "0.0.0",
  "description": "Coinbase Platform Commerce package",
  "author": "roushou <roushou9@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/roushou/coinbase-platform#readme",
  "type": "module",
